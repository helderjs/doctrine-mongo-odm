{
	"name" : "helderjs/doctrine-mongo-odm",
	"description" : "DoctrineMongoODM component for (Micro-)Framework",
	"type" : "library",
	"license" : "MIT",
	"keywords" : [
		"expressive",
		"http",
		"middleware",
		"psr",
		"psr-7",
		"doctrine",
		"mongodb",
		"odm"
	],
	"authors" : [{
			"name" : "Helder Santana",
			"email" : "contato@heldersantana.net"
		}
	],
	"require" : {
		"php" : "~5.6 || ~7.0",
		"psr/container" : "^1.0.0",
		"doctrine/mongodb-odm" : "^1.1"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^4.8 || ^5.7",
		"squizlabs/php_codesniffer" : "^2.7",
		"alcaeus/mongo-php-adapter" : "^1.0"
	},
	"autoload" : {
		"psr-4" : {
			"Helderjs\\Component\\DoctrineMongoODM\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Helderjs\\Test\\Component\\DoctrineMongoODM\\" : "test/"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.0-dev"
		}
	},
	"scripts" : {
		"check" : [
			"@cs",
			"@test"
		],
		"cs" : "vendor/bin/phpcs",
		"test" : "vendor/bin/phpunit"
	},
	"config": {
		"platform": {
			"ext-mongo": "1.6.16"
		}
	}
}
